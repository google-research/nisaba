  uname: ["DDA", "NUKTA"] raw: "𑒛𑓃"
  to_uname: "R WITH DOT BELOW" to_raw: "ṛ"
}
item {
  uname: ["DDHA", "NUKTA"] raw: "𑒜𑓃"
  to_uname: ["R WITH DOT BELOW", "MODIFIER LETTER SMALL H"] to_raw: "ṛʰ"
}
item {
  uname: ["LA", "NUKTA"] raw: "𑒪𑓃"
  to_uname: "L WITH DOT BELOW" to_raw: "ḷ"
}
item {
  uname: ["YA", "NUKTA"] raw: "𑒨𑓃"
  to_uname: "Y WITH DOT ABOVE" to_raw: "ẏ"
}
