# Letters with nukta.
item { uname: ["KA",  "NUKTA"] raw: "𑚊𑚷" to_raw: "q" }   # QA.
item { uname: ["KHA", "NUKTA"] raw: "𑚋𑚷" to_raw: "x" }   # KHHA.
item { uname: ["GHA", "NUKTA"] raw: "𑚍𑚷" to_raw: "ḡʰ" }  # GHHA.
item { uname: ["JA",  "NUKTA"] raw: "𑚑𑚷" to_raw: "z" }   # ZA.
item { uname: ["PHA", "NUKTA"] raw: "𑚟𑚷" to_raw: "f" }   # FA.
