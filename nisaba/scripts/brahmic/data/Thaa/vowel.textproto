# Thaana represents standalone vowels using `LETTER ALIFU` as a base,
# to which vowel diacritics are attached.

item { uname: ["ALIFU", "ABAFILI"]   raw: "އަ"  to_raw: "a" }
item { uname: ["ALIFU", "AABAAFILI"] raw: "އާ"  to_raw: "ā" }
item { uname: ["ALIFU", "IBIFILI"]   raw: "އި"  to_raw: "i" }
item { uname: ["ALIFU", "EEBEEFILI"] raw: "އީ"  to_raw: "ī" }
item { uname: ["ALIFU", "UBUFILI"]   raw: "އު"  to_raw: "u" }
item { uname: ["ALIFU", "OOBOOFILI"] raw: "އޫ"  to_raw: "ū" }
item { uname: ["ALIFU", "EBEFILI"]   raw: "އެ"  to_raw: "e" }
item { uname: ["ALIFU", "EYBEYFILI"] raw: "އޭ"  to_raw: "ē" }
item { uname: ["ALIFU", "OBOFILI"]   raw: "އޮ"  to_raw: "o" }
item { uname: ["ALIFU", "OABOAFILI"] raw: "އޯ"  to_raw: "ō" }
